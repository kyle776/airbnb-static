<html>
  <head>
    <meta charset="utf-8">
    <title>Airbnb home</title>
    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Open+Sans:400,300,700|Raleway:300,400,500,700">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="css/style.css">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- Leave those next 4 lines if you care about users using IE8 -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
  </head>

  <body>

    <!-- Your banner code -->
    <div class="banner" style="background: linear-gradient(-225deg, rgba(0,101,168,0.6) 0%, rgba(0,36,61,0.6) 50%), url('images/background.jpg');">
      <div class="banner-content text-center">
        <h1><strong>Welcome Home</h1></strong>
        <p>Rent out your place to make some <strong>DO$H</strong></p>
        <div>
          <%= link_to "Meet the team", "team.html", class: "btn btn-primary"%>
        </div>
      </div>
    </div>

    <!-- Your features code -->
    <div class="container">
      <div class="row">
        <div class="col-sm-3 col-xs-6">
          <!-- <img src="images/globe.png" alt="Globe"> -->
          <%= image_tag "images/globe.png"%>
          <h2>Meet people</h2>
          <p>Explore the world</p>
        </div>
        <div class="col-sm-3 col-xs-6"">
          <!-- <img src="images/beer.png" alt="Beer"> -->
          <%= image_tag "images/beer.png"%>
          <h2>Try new beer!</h2>
          <p>Don't get drunk</p>
        </div>
        <div class="col-sm-3 col-xs-6"">
          <!-- <img src="images/heart.png" alt="Heart"> -->
          <%= image_tag "images/heart.png"%>
          <h2>Fall in love</h2>
          <p>Your heart can take you anywhere</p>
        </div>
        <div class="col-sm-3 col-xs-6"">
          <!-- <img src="images/dollar.png" alt="Dollar"> -->
          <%= image_tag "images/dollar.png"%>
          <h2>Waste money</h2>
          <p>Should just stay in a resort</p>
        </div>
      </div>
    </div>

    <!-- Your cards code -->
    <div class="wrapper-grey padded">
      <div class="container">
        <h2 class="text-center">Your next trip</h2>
        <div class="row">
          <div class="col-xs-4">
            <div class="card" style="background: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.2)), url('images/card-london.jpeg');"">
              <div class="card-catagory">London</div>
              <div class="card-description">
                <h3>Ian's Home</h3>
                <p>A lovely place</p>
              </div>
              <!-- <img class="avatar" src="images/seb.jpg" alt="Seb"> -->
              <%= image_tag "images/seb.jpg", class: "avatar" %>
            </div>
          </div>
          <div class="col-xs-4">
            <div class="card" style="background: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.2)), url('images/card-paris.jpg');"">
              <div class="card-catagory">Paris</div>
              <div class="card-description">
                <h3>Romain's Home</h3>
                <p>A lovely place</p>
              </div>
              <!-- <img class="avatar" src="images/romain.jpeg" alt="Romain"> -->
              <%= image_tag "images/romain.jpg", class: "avatar"%>
            </div>
          </div>
          <div class="col-xs-4">
            <div class="card" style="background: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.2)), url('images/card-brussels.jpeg');"">
              <div class="card-catagory">Brussels</div>
              <div class="card-description">
                <h3>Anne's Home</h3>
                <p>A lovely place</p>
              </div>
              <!-- <img class="avatar" src="images/anne.jpg" alt="Anne"> -->
              <%= image_tag "images/boris.jpg", class: "avatar"%>
            </div>
          </div>
        </div>
      </div>
    </div>

    <!-- Your newsletter code -->
    <div class="text-center padded">
    <h2>Get news from us</h2>
    <p>Leave use your email to stay informed!</p>
      <form action="#" class="form-inline">
        <input type="your email" placeholder="joe@bloggs.com" class="form-control">
        <input type="submit" value="sign in" class="btn btn-primary">
      </form>
    </div>

    <!-- Your map code -->
    <div id="map" style="width: 100%; height: 400px;"></div>

<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js"></script>

<script type="text/javascript">
var myLatlng = new google.maps.LatLng(48.8655909, 2.378983);

var myOptions = {
  zoom: 16,
  center: myLatlng,
  scrollwheel: false,
  mapTypeId: google.maps.MapTypeId.ROADMAP,
  styles: [] // TODO: replace [] by array from https://snazzymaps.com/
};

var map = new google.maps.Map(document.getElementById('map'), myOptions);
var marker = new google.maps.Marker({
  position: myLatlng,
  map: map,
  title: "You are here!"
});
</script>


    <!-- Your footer code -->


    <!-- Including Bootstrap JS (with its jQuery dependency) so that dynamic components work -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script>
  </body>
</html>
